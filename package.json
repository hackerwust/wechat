    "jade": "^1.11.0",
    "mongodb": "^2.1.18",
    "socket.io": "^1.3.7",
    "source-map-support": "0.4.0",
    "stylus": "^0.54.5",
    "thinkjs": "2.2.x"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-stage-1": "6.x.x",
    "gulp-clean-css": "^2.0.11"
  },
